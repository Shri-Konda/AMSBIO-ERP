<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="select_edi_order_view_form" model="ir.ui.view">
        <field name="name">select.edi.order.view.form</field>
        <field name="model">select.edi.order</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="order_id" options="{'no_create': True}"/>
                    </group>
                </group>
                <footer>
                    <button string="Done" name="action_mark_done" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>